import abc
from abc import ABC, abstractmethod
from supertokens_python.recipe.session.interfaces import SessionContainer as SessionContainer
from typing import Any, Dict, Union

class BaseRequest(ABC, metaclass=abc.ABCMeta):
    wrapper_used: bool
    request: Any
    def __init__(self) -> None: ...
    @abstractmethod
    def get_query_param(self, key: str, default: Union[str, None] = ...) -> Union[str, None]: ...
    @abstractmethod
    async def json(self) -> Union[Any, None]: ...
    @abstractmethod
    async def form_data(self) -> Dict[str, Any]: ...
    @abstractmethod
    def method(self) -> str: ...
    @abstractmethod
    def get_cookie(self, key: str) -> Union[str, None]: ...
    @abstractmethod
    def get_header(self, key: str) -> Union[None, str]: ...
    @abstractmethod
    def get_session(self) -> Union[SessionContainer, None]: ...
    @abstractmethod
    def set_session(self, session: SessionContainer): ...
    @abstractmethod
    def get_path(self) -> str: ...
